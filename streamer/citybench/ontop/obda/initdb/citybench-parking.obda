[PrefixDeclaration]
:		http://www.streamreasoning.org/ontologies/rsplab/citybench.owl#
ct:		http://www.insight-centre.org/citytraffic#
dr:		http://www.insight-centre/DataRequest#
ces:		http://www.insight-centre.org/ces#
dul:		http://www.loa.istc.cnr.it/ontologies/DUL.owl#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
ses:		http://www.insight-centre.org/dataset/SampleEventService#
ssn:		http://purl.oclc.org/NET/ssnx/ssn#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
prof:		http://www.daml.org/services/owl-s/1.2/Profile.owl#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
service:		http://www.daml.org/services/owl-s/1.2/Service.owl#
servicep:	http://www.daml.org/services/owl-s/1.2/ServiceParameter.owl#
grounding:	http://www.daml.org/services/owl-s/1.2/Grounding.owl#



[MappingDeclaration] @collection [[

mappingId	AarhusParkingVacanciesObservation-AarhusParkingDataBRUUNS
target		:/AarhusParkingDataBRUUNS/{ID}-{RANDOM} a ssn:Observation ; :eventTime {TIMESTAMP}^^xsd:dateTime ; ct:hasValue {vacancies}^^xsd:long ; ssn:observedProperty ses:{PID} ; ssn:observedBy ses:AarhusParkingDataBRUUNS . 
source		select  _id as id, setseed(_id::int * 0.00000001), md5(random()::text) as random, "updatetime"::timestamp AS TIMESTAMP, "totalspaces"::int - "vehiclecount"::int as vacancies, sp."property" as PID
			from "AarhusParkingDataBRUUNS", "service_properties" as sp
			WHERE sp."service"='AarhusParkingDataBRUUNS'

]]

