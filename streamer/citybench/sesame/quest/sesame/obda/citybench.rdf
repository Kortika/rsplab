     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:prof="http://www.daml.org/services/owl-s/1.2/Profile.owl#"
     xmlns:servicep="http://www.daml.org/services/owl-s/1.2/ServiceParameter.owl#"
     xmlns:dr="http://www.insight-centre/DataRequest#"
     xmlns:ssn="http://purl.oclc.org/NET/ssnx/ssn#"
     xmlns:ces="http://www.insight-centre.org/ces#"
     xmlns:ct="http://www.insight-centre.org/citytraffic#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:service="http://www.daml.org/services/owl-s/1.2/Service.owl#"
     xmlns:dul="http://www.loa.istc.cnr.it/ontologies/DUL.owl#"
     xmlns:DUL="http://www.loa-cnr.it/ontologies/DUL.owl#"
     xmlns:grounding="http://www.daml.org/services/owl-s/1.2/Grounding.owl#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://www.streamreasoning.org/ontologies/rsplab/citybench.owl">
        <owl:imports rdf:resource="http://www.loa.istc.cnr.it/ontologies/DUL.owl"/>
        <dc:title>Citybench Ontology</dc:title>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This ontology is the one used to annotate data for the citybench RDF Stream Processing Benchmark. It contains classes from the following ontologies: Semantic Senso Network, Dolce, DAML service, service category, service parameter and profile, process, grounding. It also integrates the following vocabularies, that were previously provided in separated files from the insight-centre: Service Parameter, Ces, data request.</rdfs:comment>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://citypulse.insight-centre.org/ontology/ces/</rdfs:seeAlso>
    </owl:Ontology>
    


