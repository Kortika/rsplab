tw$stream_name:
    labels:
      stream.name: $stream
      stream.type: $stream_type
      stream.uuid: $uuid
      stream.run.uudi: $run_uuid
    container_name: $stream
    command: running/$stream.properties
    build: ./
    ports:
      - "$port:$port"
      - "$ws_port:$ws_port"
    volumes:
      - ./:/opt/tw/running